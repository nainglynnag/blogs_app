    <script>
        // Back button functionality
        function goBack() {
            window.history.back();
        }

        // Like button functionality
        const likeBtn = document.querySelector('button:has(span:contains("üëç"))');
        let isLiked = false;
        let likeCount = 42;

        if (likeBtn) {
            likeBtn.addEventListener('click', function() {
                isLiked = !isLiked;
                if (isLiked) {
                    likeCount++;
                    this.classList.remove('bg-sky-100', 'hover:bg-sky-200', 'text-sky-600');
                    this.classList.add('bg-sky-500', 'text-white');
                } else {
                    likeCount--;
                    this.classList.remove('bg-sky-500', 'text-white');
                    this.classList.add('bg-sky-100', 'hover:bg-sky-200', 'text-sky-600');
                }
                this.querySelector('span:last-child').textContent = `Like (${likeCount})`;
            });
        }

        // Bookmark functionality
        const bookmarkBtn = document.querySelector('button:has(span:contains("üîñ"))');
        let isBookmarked = false;

        if (bookmarkBtn) {
            bookmarkBtn.addEventListener('click', function() {
                isBookmarked = !isBookmarked;
                if (isBookmarked) {
                    this.classList.remove('bg-yellow-100', 'hover:bg-yellow-200', 'text-yellow-600');
                    this.classList.add('bg-yellow-500', 'text-white');
                    this.querySelector('span:last-child').textContent = 'Bookmarked';
                } else {
                    this.classList.remove('bg-yellow-500', 'text-white');
                    this.classList.add('bg-yellow-100', 'hover:bg-yellow-200', 'text-yellow-600');
                    this.querySelector('span:last-child').textContent = 'Bookmark';
                }
            });
        }

        // Comment functionality
        const commentForm = document.querySelector('textarea');
        const postCommentBtn = document.querySelector('button:contains("Post Comment")');

        if (postCommentBtn && commentForm) {
            postCommentBtn.addEventListener('click', function() {
                const commentText = commentForm.value.trim();
                if (commentText) {
                    // Simulate posting comment
                    this.textContent = 'Posting...';
                    setTimeout(() => {
                        this.textContent = 'Post Comment';
                        commentForm.value = '';
                        alert('Comment posted successfully!');
                    }, 1000);
                }
            });
        }

        // Comment like functionality
        document.querySelectorAll('button:contains("üëç Like")').forEach(btn => {
            btn.addEventListener('click', function() {
                const currentText = this.textContent;
                const match = currentText.match(/\((\d+)\)/);
                if (match) {
                    const currentCount = parseInt(match[1]);
                    const newCount = currentCount + 1;
                    this.textContent = currentText.replace(/\(\d+\)/, `(${newCount})`);
                    this.classList.add('text-sky-600');
                }
            });
        });

        // Share functionality
        const shareBtn = document.querySelector('button:has(span:contains("üîó"))');
        if (shareBtn) {
            shareBtn.addEventListener('click', function() {
                if (navigator.share) {
                    navigator.share({
                        title: 'The Future of Web Development: Trends to Watch',
                        text: 'Check out this amazing article about web development trends!',
                        url: window.location.href
                    });
                } else {
                    // Fallback for browsers that don't support Web Share API
                    navigator.clipboard.writeText(window.location.href).then(() => {
                        alert('Link copied to clipboard!');
                    });
                }
            });
        }

        // Follow button functionality
        const followBtn = document.querySelector('button:contains("Follow")');
        let isFollowing = false;

        if (followBtn) {
            followBtn.addEventListener('click', function() {
                isFollowing = !isFollowing;
                if (isFollowing) {
                    this.textContent = 'Following';
                    this.classList.remove('bg-sky-500', 'hover:bg-sky-600');
                    this.classList.add('bg-green-500', 'hover:bg-green-600');
                } else {
                    this.textContent = 'Follow';
                    this.classList.remove('bg-green-500', 'hover:bg-green-600');
                    this.classList.add('bg-sky-500', 'hover:bg-sky-600');
                }
            });
        }

        // Smooth scroll for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth'
                    });
                }
            });
